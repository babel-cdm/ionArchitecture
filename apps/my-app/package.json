{
  "name": "my-app",
  "scripts": {
    "mock:start": "npx mockoon-cli start --data ./src/mock/api-mock.json",
    "mock:list": "npx mockoon-cli list",
    "mock:stop": "npx mockoon-cli stop",

    "sb:start": "npx ng run my-app:storybook",
    "sb:build": "storybook build",

    "ion:open": "ionic cap open --project=my-app",

    "ion:serve:mock": "ionic serve --project=my-app",
    "ion:serve:pre": "ionic serve --configuration=preproduction --project=my-app",

    "ion:run:ios:mock": "ionic cap run ios --livereload --external --project=my-app",
    "ion:run:ios:pre": "ionic cap run ios --configuration=preproduction --livereload --external --project=my-app",
    "ion:run:android:mock": "ionic cap run android --livereload --external --project=my-app",
    "ion:run:android:pre": "ionic cap run android --configuration=preproduction -livereload --external --project=my-app",

    "ion:build:ios:mock": "ionic capacitor build ios --project=my-app",
    "ion:build:ios:pre": "ionic capacitor build ios --configuration=preproduction --project=my-app",
    "ion:build:ios:prod": "ionic capacitor build ios --configuration=production --prod --release --project=my-app",
    "ion:build:android:mock": "ionic capacitor build android --project=my-app",
    "ion:build:android:pre": "ionic capacitor build android --configuration=preproduction --project=my-app",
    "ion:build:android:prod": "ionic capacitor build android --configuration=production --prod --release --project=my-app"
  },
  "dependencies": {
    "@capacitor/android": "4.6.1",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.6.1",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.6.1",
    "@capacitor/keyboard": "4.1.0",
    "@capacitor/status-bar": "4.1.1"
  },
  "devDependencies": {
    "@capacitor/cli": "4.6.1"
  }
}
