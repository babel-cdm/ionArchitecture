{
  "name": "my-app",
  "projectType": "application",
  "sourceRoot": "apps/my-app/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
          "command": "npx ng build my-app"
      },
      "configurations": {
          "development": {
               "cmd": "npx ng build my-app -c=development"
          },
          "production": {
              "cmd": "npx ng build my-app -c=production"
          }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
          "command": "npx ng serve my-app"
      },
      "configurations": {
          "development": {
               "cmd": "npx ng serve my-app -c=development"
          },
          "production": {
              "cmd": "npx ng serve my-app -c=production"
          }
      },
      "defaultConfiguration": "development"
    }
  },
  "tags": []
}
